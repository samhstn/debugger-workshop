{
  "name": "autocomplete-app",
  "version": "1.0.0",
  "description": "A word auto-completion project",
  "main": "server.js",
  "scripts": {
    "nyantest": "tape backendTest/**/*.test.js",
    "check-coverage": "istanbul check-coverage --statements 20 --branches 20 --functions 20 --lines 20",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "istanbul cover tape backendTest/**/*.test.js",
    "start": "nodemon src/server.js",
    "watch": "nodemon -q -x 'npm run nyantest'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FloatingIntegers/autocomplete-app.git"
  },
  "keywords": [
    "Auto-completion",
    "Search"
  ],
  "author": "FloatingIntegers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FloatingIntegers/autocomplete-app/issues"
  },
  "homepage": "https://github.com/FloatingIntegers/autocomplete-app#readme",
  "dependencies": {
    "shot": "^3.1.0",
    "split": "1.0.0",
    "tape": "^4.6.0"
  },
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^0.4.4",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.4",
    "tap-nyan": "0.0.2",
    "tap-spec": "^2.2.2",
    "tape": "^4.6.0"
  },
  "engines": {
    "node": "6.2.2"
  }
}
